<?xml version="1.0" encoding="UTF-8"?>
<factura xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="xmlView.xsd">
    <id th:text="${factura.id}" />
    <fecha th:text="${#dates.format(factura.date, 'yyyy-MM-dd')}" />
    <proveedor th:text="${factura.cedulaProveedor}" />
    <cliente th:text="${factura.cedulaCliente}" />
    <tipoPago th:text="${factura.tipoPago}" />
    <precioFinal th:text="${factura.finalPrice}" />
</factura>
